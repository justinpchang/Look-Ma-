{
  "manifest_version": 2,

  "name": "Look Ma!",
  "version": "1.0.0",
  "description": "No hands!",

  "browser_action": {
    "default_icon": "icon.png"
  },

  "content_scripts": [{
    "js": ["jquery-3.1.1.min.js", "content.js"],
    "matches": ["<all_urls>"]
  }],

  "permissions": [
     "tabs", "http://*/*", "https://*/*"
  ]
}
